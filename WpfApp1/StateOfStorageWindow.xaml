<Window x:Class="WpfApp1.StateOfStorageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="StateOfStorageWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dgStateOfStorage" HorizontalAlignment="Left" Height="266" Margin="129,83,0,0" VerticalAlignment="Top" Width="571"  AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="StateOfStorageId" Binding="{Binding Path=StateOfStorageId}"/>
                <DataGridTextColumn Header="ProductId" Binding="{Binding Path=ProductId}"/>
                <DataGridTextColumn Header="StorageId" Binding="{Binding Path=StorageId}"/>
                <DataGridTextColumn Header="Quantity" Binding="{Binding Path=Quantity}"/>


            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
